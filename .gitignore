###### search results display for lang and specialties

<div>
  <%= s.collection_check_boxes(:specialties, Specialty.all, :id, :ability, {:label => 'Specialty'}) %>
</div>

<div>
  <%= s.collection_check_boxes(:languages, Language.all, :id, :sglang, {:label => 'Language '}) %>
</div>

<div class="field">
  <%= s.label :specialties %>
  <%= s.select :specialties, options_for_select(@specialty), :include_blank => true %>
</div>

<div class="field">
  <%= s.label :language %>
  <%= s.select :languages, options_for_select(@lang), :include_blank => true %>
</div>





<% if @search.search_caregivers_languages.blank? %>
<% elsif @search.search_caregivers_languages.each do |c| %>
<div class="div">
  Language: <%= c.sglang %><br>
</div>

  <% end %>
<% end %>
<% if @search.search_caregivers_specialties.blank? %>
<% elsif @search.search_caregivers_specialties.each do |c| %>
<div class="div">
  Specialties: <%= c.ability %><br>
</div>
<% end %>
<% end %>





