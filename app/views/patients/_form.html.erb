<%= bootstrap_form_for(patient, url:  fammember_patients_path, html: { method: :post }) do |f| %>

  <div class="field">
    name of fammember: <%= current_user.first_name %>
  </div>

  <div class="field">

    <%= f.text_field :first_name %>
  </div>

  <div class="field">

    <%= f.text_field :last_name %>
  </div>

  <div class="field">

    <%= f.text_field :address %>
  </div>

  <div class="field">

    <%= f.text_field :gender %>
  </div>

  <div class="field">

    <%= f.text_field :frequency %>
  </div>

  <div class="field">

    <%= f.text_field :condition_description %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
